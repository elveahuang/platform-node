{
    "extends": "@strapi/typescript-utils/tsconfigs/server",
    "compilerOptions": {
        "allowJs": true,
        "outDir": "dist",
        "rootDir": ".",
        "baseUrl": ".",
        "paths": {
            "@/*": ["./src/*"]
        }
    },
    "include": ["./", "./**/*.ts", "./**/*.js", "src/**/*.json"],
    "exclude": ["node_modules/", "build/", "dist/", ".cache/", ".tmp/", "src/admin/", "**/*.test.*", "src/plugins/**"]
}
